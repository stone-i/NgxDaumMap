{
  "name": "ngx-daum-map",
  "version": "0.3.42",
  "description": "DaumMap components for Angular2 or Higher",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular2",
    "ng",
    "daum-maps",
    "daum",
    "ng2",
    "maps"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf .tmp && mkdir dist && mkdir .tmp && rimraf 'src/**/*.ngfactory.ts'",
    "prebuild": "npm run clean",
    "build": "npm run scripts && npm run packagejson && npm run copyinfo",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "packagejson": "node ./scripts/create-package-json.js",
    "scripts": "npm run ngc:esm && npm run bundle:umd",
    "copyinfo": "cp ./README.md ./dist/README.md",
    "bundle:umd": "rollup -c rollup.config.js",
    "ngc:esm": "ngc -p tsconfig.json",
    "clang:format": "clang-format --glob=src/**/*.ts -i",
    "postngc:esm": "mkdir -p dist/ && cp -R .tmp/esm/src/* dist/ && rimraf src/**/*.ngfactory.ts",
    "test": "karma start",
    "test:watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run build && npm run test",
    "auto-pub": "npm run build && npm publish dist"
  },
  "peerDependencies": {
    "@angular/common": "^4.0.0 || ^2.0.0",
    "@angular/core": "^4.0.0 || ^2.0.0"
  },
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/animations": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/tsc-wrapped": "4.0.0",
    "@ngtools/webpack": "1.3.0",
    "@types/jasmine": "2.5.46",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "3.1.2",
    "clang-format": "1.0.46",
    "codelyzer": "3.0.0-beta.4",
    "conventional-changelog": "^1.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.8.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "2.5.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-remap-istanbul": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "reflect-metadata": "^0.1.8",
    "remap-istanbul": "^0.9.1",
    "rimraf": "2.6.1",
    "rollup": "0.41.6",
    "rxjs": "^5.2.0",
    "ts-helpers": "^1.1.2",
    "tslint": "~4.5.1",
    "tslint-loader": "^3.4.3",
    "typescript": "~2.2.0",
    "webpack": "2.3.3",
    "zone.js": "^0.8.4"
  },
  "jspm": {
    "jspmNodeConversion": false,
    "dependencies": {
      "@angular/common": "^4.0.0 || ^2.0.0",
      "@angular/core": "^4.0.0 || ^2.0.0"
    }
  }
}
